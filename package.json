{
  "name": "@jungvonmatt/contentful-migrations",
  "version": "2.0.1",
  "description": "Helper to handle migrations in contentful",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "bin": {
    "contentful-migrations": "index.js",
    "migrations": "index.js"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint .  --ext .js,.ts,.tsx --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jungvonmatt/contentful-migrations.git"
  },
  "author": "Ben ZÃ¶rb <benjamin.zoerb@jvm.de>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "contentful",
    "migration",
    "content-type"
  ],
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^14.1.2",
    "array.prototype.flatmap": "^1.2.4",
    "ascii-tree": "^0.3.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "common-tags": "^1.8.0",
    "contentful-cli": "^1.5.14",
    "contentful-import": "^7.9.33",
    "contentful-management": "^7.5.2",
    "contentful-migration": "^4.0.5",
    "cosmiconfig": "^7.0.0",
    "deep-diff": "^1.0.2",
    "diff": "^5.0.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.1.0",
    "git-branch": "^2.0.1",
    "globby": "^11.0.2",
    "inquirer": "^7.3.3",
    "markdown-table": "^2.0.0",
    "merge-options": "^3.0.4",
    "node-fetch": "^2.6.1",
    "pkg-up": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "extends": [
      "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "ecmaVersion": 2017,
        "impliedStrict": true
      }
    },
    "env": {
      "node": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-path .gitignore",
      "eslint --fix --ignore-path .gitignore"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
